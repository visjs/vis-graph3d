{
  "name": "vis-graph3d",
  "version": "0.0.0-no-version",
  "description": "Create interactive, animated 3d graphs. Surfaces, lines, dots and block styling out of the box.",
  "main": "dist/vis-graph3d.min.js",
  "module": "dist/esm.js",
  "homepage": "https://visjs.github.io/vis-graph3d/",
  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/visjs/vis-graph3d.git"
  },
  "bugs": {
    "url": "https://github.com/visjs/vis-graph3d/issues"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/visjs"
  },
  "keywords": [
    "vis",
    "visualization",
    "web based",
    "browser based",
    "javascript",
    "chart",
    "graph",
    "browser"
  ],
  "files": ["**/*"],
  "scripts": {
    "test": "BABEL_ENV=test mocha --exit --require @babel/register",
    "test-cov": "BABEL_ENV=test-cov nyc --reporter=lcov mocha --exit --require @babel/register",
    "js:graph3d": "rollup --config rollup.config.js",
    "build": "npm run js:graph3d",
    "watch": "rollup --watch --config rollup.config.js",
    "watch-dev": "npm run watch-dev",
    "generate-examples-index": "generate-examples-index -t \"Vis Graph3D Examples\" -c mygraph -w https://visjs.github.io/vis-graph3d -S ./scripts/examples-index/screenshot-script.js -lis",
    "lint": "eslint '{src,test}/**/*.{m,}{j,t}s{x,}'",
    "clean": "rimraf 'dist/*'",
    "postinstall": "opencollective postinstall || exit 0",
    "prepublishOnly": "npm run contributors:update && npm run build",
    "contributors:update": "git-authors-cli"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "emitter-component": "1.1.1",
    "hammerjs": "2.0.8",
    "keycharm": "0.3.0",
    "moment": "2.24.0",
    "propagating-hammerjs": "1.4.7",
    "timsort": "0.3.0",
    "vis-data": "6.3.2",
    "vis-util": "2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime-corejs3": "^7.7.7",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "eslint": "^6.1.0",
    "git-authors-cli": "^1.0.18",
    "husky": "^4.0.2",
    "jsdom": "15.2.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.2.3",
    "mocha": "^6.2.0",
    "nyc": "^15.0.0",
    "opencollective": "^1.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "semantic-release": "^16.0.0",
    "sinon": "^8.0.0",
    "test-console": "^1.1.0",
    "vis-dev-utils": "^1.2.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/visjs"
  }
}
