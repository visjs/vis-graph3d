{
  "name": "@jimp/core",
  "version": "0.8.5",
  "description": "Jimp core",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "es",
    "index.d.ts",
    "fonts",
    "types"
  ],
  "repository": {
    "type": "git",
